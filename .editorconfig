root = true
[*]
end_of_line = lf                    # 行尾 UNIX 格式 LF
charset = utf-8-bom                 # 文件编码字符集为 UTF-8
trim_trailing_whitespace = true     # 删除文件末尾空格
insert_final_newline = true         # 末尾插入新行
indent_style = space                # 使用空格缩进
indent_size = 4                     # 缩进大小为 4 个空格
